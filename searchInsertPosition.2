//more concise version, but may cost more time than version 1
public class Solution {
    public int searchInsert(int[] A, int target) {
        int len  = A.length;
        int low = 0;
        int high = len-1;
        int mid;
        while(low <= high)
        {
            mid = (low + high) / 2;
            if(target == A[mid])
                return mid;
            if(target < A[mid])
                high = mid - 1;
            else if(target > A[mid])
                low = mid + 1;
        }
        return low;
    }
}